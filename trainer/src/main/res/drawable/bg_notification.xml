<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"

    >

  <item
      android:gravity="center"
      android:height="16dp"
      android:width="16dp">
    <shape
        android:shape="oval"
        >
      <size
          android:height="15dp"
          android:width="15dp"/>
      <solid android:color="@color/red"/>
      <stroke
          android:color="@color/white"
          android:width="1dp"/>
    </shape>

  </item>


</layer-list>
