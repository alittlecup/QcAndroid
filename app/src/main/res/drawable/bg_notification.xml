<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"

    >
    <item
        android:width="11dp"
        android:height="11dp">
        <shape android:shape="oval">
            <size
                android:width="15dp"
                android:height="15dp"/>
            <solid android:color="@color/primary"/>
        </shape>
    </item>
    <item
        android:width="10dp"
        android:height="10dp"
        android:gravity="center">
        <shape
            android:shape="oval"
            >
            <size
                android:width="10dp"
                android:height="10dp"/>
            <solid android:color="@color/red"/>
            <stroke
                android:width="1dp"
                android:color="@color/white"/>
        </shape>

    </item>


</layer-list>
