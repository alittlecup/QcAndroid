<resources>
  <declare-styleable name="TagGroup">
    <!-- Whether the tag group is in append mode. -->
    <attr format="boolean" name="atg_isAppendMode"/>
    <!-- If the tag group is in append mode, what the hint of input tag. -->
    <attr format="string" name="atg_inputHint"/>

    <!-- The tag view outline border color. -->
    <attr format="color" name="atg_borderColor"/>
    <!-- The tag view text color. -->
    <attr format="color" name="atg_textColor"/>
    <!-- The tag view background color. -->
    <attr format="color" name="atg_backgroundColor"/>
    <attr format="reference" name="atg_backgroundRes"/>

    <!-- The dash outline border color, when in append mode. -->
    <attr format="color" name="atg_dashBorderColor"/>
    <!-- The input tag hint text color, when in append mode. -->
    <attr format="color" name="atg_inputHintColor"/>
    <!-- The input tag type text color, when in append mode. -->
    <attr format="color" name="atg_inputTextColor"/>

    <!-- The checked tag view outline border color. -->
    <attr format="color" name="atg_checkedBorderColor"/>
    <!-- The checked text color. -->
    <attr format="color" name="atg_checkedTextColor"/>
    <!-- The checked marker color. -->
    <attr format="color" name="atg_checkedMarkerColor"/>
    <!-- The checked tag view background color. -->
    <attr format="color" name="atg_checkedBackgroundColor"/>

    <!-- The tag view background color, when the tag view is being pressed. -->
    <attr format="color" name="atg_pressedBackgroundColor"/>

    <!-- The tag view outline border stroke width. -->
    <attr format="dimension" name="atg_borderStrokeWidth"/>
    <!-- The tag view text size. -->
    <attr format="dimension" name="atg_textSize"/>
    <attr format="integer" name="atg_color_count"/>

    <attr format="dimension" name="atg_horizontalSpacing"/>
    <attr format="dimension" name="atg_verticalSpacing"/>
    <attr format="dimension" name="atg_horizontalPadding"/>
    <attr format="dimension" name="atg_verticalPadding"/>
  </declare-styleable>


  <declare-styleable name="CommonInputView">
    <attr format="string" name="civ_lable"/>
    <attr format="string" name="civ_content"/>
    <attr format="boolean" name="civ_clickable"/>
    <attr format="boolean" name="civ_inputnum"/>
    <attr format="boolean" name="civ_nonnull"/>
    <attr format="reference" name="civ_right_icon"/>
    <attr format="boolean" name="civ_showdivier"/>
    <attr format="boolean" name="civ_showright"/>
    <attr format="integer" name="civ_max_length"/>
    <attr format="integer" name="civ_text_color"/>
    <attr format="string" name="civ_hint"/>

  </declare-styleable>

  <declare-styleable name="Themes">
    <attr format="reference" name="tagGroupStyle"/>
  </declare-styleable>

  <declare-styleable name="RobotoCalendarView">
    <attr format="color" name="monthTitleColor"/>
    <attr format="string" name="monthTitleFont"/>
    <attr format="color" name="dayOfWeekColor"/>
    <attr format="string" name="dayOfWeekFont"/>
    <attr format="color" name="dayOfMonthColor"/>
    <attr format="string" name="dayOfMonthFont"/>
  </declare-styleable>
  <declare-styleable name="RobotoTextView">
    <attr name="font"/>
  </declare-styleable>
  <declare-styleable name="RobotoFonts">
    <attr format="enum" name="font">
      <enum name="roboto_thin" value="0"/>
      <enum name="roboto_thin_italic" value="1"/>
      <enum name="roboto_light" value="2"/>
      <enum name="roboto_light_italic" value="3"/>
      <enum name="roboto_regular" value="4"/>
      <enum name="roboto_italic" value="5"/>
      <enum name="roboto_medium" value="6"/>
      <enum name="roboto_medium_italic" value="7"/>
      <enum name="roboto_bold" value="8"/>
      <enum name="roboto_bold_italic" value="9"/>
      <enum name="roboto_black" value="10"/>
      <enum name="roboto_black_italic" value="11"/>
      <enum name="roboto_condensed_light" value="12"/>
      <enum name="roboto_condensed_light_italic" value="13"/>
      <enum name="roboto_condensed_regular" value="14"/>
      <enum name="roboto_condensed_italic" value="15"/>
      <enum name="roboto_condensed_bold" value="16"/>
      <enum name="roboto_condensed_bold_italic" value="17"/>
      <enum name="roboto_slab_thin" value="18"/>
      <enum name="roboto_slab_light" value="19"/>
      <enum name="roboto_slab_regular" value="20"/>
      <enum name="roboto_slab_bold" value="21"/>
    </attr>
  </declare-styleable>

  <declare-styleable name="FloatingActionButton">
    <attr format="color" name="fab_colorPressed"/>
    <attr format="color" name="fab_colorDisabled"/>
    <attr format="color" name="fab_colorNormal"/>
    <attr format="reference" name="fab_icon"/>
    <attr format="enum" name="fab_size">
      <enum name="normal" value="0"/>
      <enum name="mini" value="1"/>
    </attr>
    <attr format="string" name="fab_title"/>
    <attr format="boolean" name="fab_stroke_visible"/>
  </declare-styleable>
  <declare-styleable name="AddFloatingActionButton">
    <attr format="color" name="fab_plusIconColor"/>
  </declare-styleable>
  <declare-styleable name="FloatingActionsMenu">
    <attr format="color" name="fab_addButtonColorPressed"/>
    <attr format="color" name="fab_addButtonColorNormal"/>
    <attr format="enum" name="fab_addButtonSize">
      <enum name="normal" value="0"/>
      <enum name="mini" value="1"/>
    </attr>
    <attr format="color" name="fab_addButtonPlusIconColor"/>
    <attr format="boolean" name="fab_addButtonStrokeVisible"/>
    <attr format="reference" name="fab_labelStyle"/>
    <attr format="enum" name="fab_labelsPosition">
      <enum name="left" value="0"/>
      <enum name="right" value="1"/>
    </attr>
    <attr format="enum" name="fab_expandDirection">
      <enum name="up" value="0"/>
      <enum name="down" value="1"/>
      <enum name="left" value="2"/>
      <enum name="right" value="3"/>
    </attr>
  </declare-styleable>


  <declare-styleable name="CircleIndicator">
    <attr format="dimension" name="ci_width"/>
    <attr format="dimension" name="ci_height"/>
    <attr format="dimension" name="ci_margin"/>
    <attr format="reference" name="ci_animator"/>
    <attr format="reference" name="ci_animator_reverse"/>
    <attr format="reference" name="ci_drawable"/>
    <attr format="reference" name="ci_drawable_unselected"/>
    <attr format="integer" name="ci_count_less"/>
  </declare-styleable>

  <declare-styleable name="PagerSlidingTabStrip">
    <attr format="color" name="pst_text_color"/>
    <attr format="dimension" name="pst_text_size"/>
    <attr format="color" name="pst_text_highlight_color"/>
  </declare-styleable>


</resources>