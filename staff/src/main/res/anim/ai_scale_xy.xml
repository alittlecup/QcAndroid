<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
  <objectAnimator
      android:duration="1500"
      android:pivotX="0.5"
      android:pivotY="0.5"
      android:propertyName="scaleX"
      android:valueFrom="0"
      android:valueTo="1"
      android:valueType="floatType"
      />
  <objectAnimator
      android:duration="1500"
      android:pivotX="0.5"
      android:pivotY="0.5"
      android:propertyName="scaleY"
      android:valueFrom="0"
      android:valueTo="1"
      android:valueType="floatType"
      />
</set>
